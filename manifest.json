{
  "manifest_version": 2,

  "name": "Currency exchange rates",
  "description": "Currency exchange rates plugin",
  "short_name": "Exchange rates",
  "version": "1.1",
   "icons": { 
    "64": "img/dollar.png"
  },

  "browser_action": {
    "default_icon": "img/dollar.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "https://btc-e.com/api/2/*",
    "https://query.yahooapis.com/v1/public/*"
  ]
}